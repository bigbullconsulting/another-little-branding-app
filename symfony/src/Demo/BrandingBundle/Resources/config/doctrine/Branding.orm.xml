<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Demo\BrandingBundle\Entity\Branding" table="branding">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hostKey" type="string" column="host_key" length="40" nullable="true"/>
    <field name="name" type="string" column="name" length="80" nullable="true"/>
    <field name="urlLogoImage" type="string" column="url_logo_image" length="200" nullable="true"/>
    <field name="urlHome" type="string" column="url_home" length="100" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
  </entity>
</doctrine-mapping>
